.doughnut_chart_container {
  backdrop-filter: blur(5px);
  box-sizing: border-box;
  .inner_wrap {
    position: relative;
    width: 100%;
    height: 100%;
    .chart_bg {
      width: 100%;
      height: 100%;
      z-index: -1;
      position: absolute;
      top: 0;
      border-radius: 50%;
    }
  }
}
